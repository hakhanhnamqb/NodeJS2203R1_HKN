<div class="page-heading">
  <h1>Create a new book</h1>
</div>
<form action="/book/createBook" method="POST" enctype="multipart/form-data">
  <table class=”control-group”>
      <tr class="form-group floating-label-form-group controls">
          <td><label>Book Name</label></td>
          <td><input type="text" class="form-control" id="bookName" name="bookName"></td>
      </tr>
      <tr class="form-group floating-label-form-group controls">
          <td><label>Price</label></td>
          <td><input type="number" class="form-control" id="price" name="price"></td>
      </tr>
      <tr class="form-group floating-label-form-group controls">
          <td><label>Library Name</label></td>
          <!-- <td><input type="text" class="form-control" id="libraryID" name="libraryID"></td> -->
          <td>
            <select class="libraryID" data-style="btn-info" name="libraryID">
                <optgroup label="Select Library">
                    <% libraris.forEach(function(library){ %>
                    <option value="<%= library.id %>"><%= library.libraryName %></option>
                    <% }) %>
                </optgroup>
            </select>
          </td>
      </tr>
      <tr class="form-group floating-label-form-group controls">
          <td><label>Quantity</label></td>
          <td><input type="number" class="form-control" id="quantity" name="quantity"></td>
      </tr>
      <tr class="form-group floating-label-form-group controls">
          <td><label>Author</label></td>
          <td><input type="text" class="form-control" id="author" name="author"></td>
      </tr>
  </table>
  <div class="form-group">
      <button type="submit" class="btn btn-primary">Register</button>
  </div>
</form>