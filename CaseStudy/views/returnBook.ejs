<script>
    const borrowBookModel = require('../model/borrowBokModel');
    const returnbook = async (id)=>{
        await borrowBookModel.findone()
    }
</script>
<div class="page-heading">
    <h1>Borrow a book</h1>
</div>
<form action="/borrow/returnBook" method="POST" enctype="multipart/form-data">
    <% for (const borrow of borrowed){ %> 
    <table class=”control-group”>
        <tr class="form-group floating-label-form-group controls">
            <td><label>Student Name</label></td>
            <td><p><%= borrow.nameStudent %></p></td>
        </tr>
        <tr class="form-group floating-label-form-group controls">
            <td><label>Student ID</label></td>
            <td><p><%= borrow.studentID %></p></td>
        </tr>
        <tr>
            <td><label>Quantity Borrow</label></td>
            <td><td><p><%= borrow.quantity_borrow %></p></td></td>
        </tr>
        <tr class="form-group floating-label-form-group controls">
            <td><label>Borrow Date</label></td>
            <td><td><p><%= borrow.borrowDate %></p></td></td>
        </tr>
        <tr>
            <button onclick="returnbook('<%= borrow.id %>')">RETURNING</button>
            <button onclick="">DELETE</button>
        </tr>
    </table>
    <% } %>
</form>