<div class="page-heading">
        <h1>Create a Library</h1>
    </div>
    <form action="/library/createLibrary" method="POST" enctype="multipart/form-data">
        <table>
            <tr>
                <td><label>Name Library</label></td>
                <td><input type="text" class="form-control" id="libraryName" name="libraryName"></td>
            </tr>
            <tr>
                <td><label>Librarian</label></td>
                <td><input type="text" class="form-control" id="librarian" name="librarian"></td>
            </tr>
        </table>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Register</button>
        </div>
    </form>